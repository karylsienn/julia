function encode(s)
    encoded = ""
    count = 1
    prev = ''
    for x in s
        if prev==x & !isempty(prev)
            count += 1
        else
            if count â‰¥ 2
                encoded*=string(count)*prev
            else
                encode*=prev
            end
            count = 1
        end
    end
    return(encoded)
end



